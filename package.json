{
  "name": "filament-lang",
  "version": "0.5.2",
  "description": "filament language",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/module.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=15.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist/ build/ output/ node_modules/ src/filament.ohm.js filament-lang-*.tgz",
    "compile-ohm": "tools/compile_ohm.js",
    "compile-api": "node tools/compile_api.js",
    "build-umd": "npm run compile-ohm && npm run compile-api && mkdir -p dist/",
    "build": "parcel build",
    "test-umd": "cd test-umd && rm -f dist && ln -s ../dist && ../node_modules/.bin/http-server -c-1 -o",
    "test": "npm run compile-ohm && mocha -t 200000",
    "docs": "npm run compile-ohm && node tools/alldocs",
    "exec": "node tools/exec.js",
    "compile": "tsc"
  },
  "author": "joshua@marinacci.org",
  "license": "ISC",
  "dependencies": {
    "date-fns": "^2.19.0",
    "node-fetch": "^2.6.1",
    "ohm-js": "^15.5.0",
    "pureimage": "^0.2.7"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.6.2",
    "@parcel/transformer-typescript-types": "^2.6.2",
    "http-server": "^0.12.3",
    "mocha": "^8.3.1",
    "parcel": "^2.6.2",
    "process": "^0.11.10",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
